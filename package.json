{
  "name": "react-redux-antd-server-render",
  "version": "1.0.0",
  "description": "A boilerplate includes react redux antd flexbox and server render support",
  "author": {
    "name": "Krit Hoonsuwan",
    "email": "suns_stat@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/durunvo/react-redux-antd-server-render.git"
  },
  "bugs": {
    "url": "https://github.com/durunvo/react-redux-antd-server-render/issues"
  },
  "keywords": [
    "css modules",
    "flexbox",
    "grid",
    "react",
    "antd",
    "ssr",
    "server render",
    "redux",
    "hot reload",
    "express"
  ],
  "license": "MIT",
  "scripts": {
    "start": "better-npm-run start-prod",
    "dev": "better-npm-run start-dev",
    "postinstall": "better-npm-run build:prod"
  },
  "betterScripts": {
    "build:prod": {
      "command": "webpack --config webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "start-prod": {
      "command": "node ./bin/index.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "start-dev": {
      "command": "webpack --config webpack.devServer.config.js && babel-node ./bin/index.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    }
  },
  "dependencies": {
    "antd": "^2.8.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "better-npm-run": "0.0.13",
    "compression": "^1.6.1",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "0.23.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "node-sass": "^3.8.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.8",
    "react-transform-hmr": "^1.0.0",
    "redux": "^3.0.0",
    "redux-logger": "^2.0.2",
    "redux-saga": "^0.14.3",
    "sass-loader": "^3.2.1",
    "serialize-javascript": "1.1.2",
    "style-loader": "^0.13.2",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0",
    "webpack-node-externals": "^1.5.4",
    "whatwg-fetch": "^1.0.0"
  }
}
