{
  "name": "react-redux-antd-server-render",
  "version": "1.0.0",
  "description": "A boilerplate includes react redux antd flexbox and server render support",
  "author": {
    "name": "Krit Hoonsuwan",
    "email": "suns_stat@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/durunvo/react-redux-antd-server-render.git"
  },
  "bugs": {
    "url": "https://github.com/durunvo/react-redux-antd-server-render/issues"
  },
  "keywords": [
    "css modules",
    "flexbox",
    "grid",
    "react",
    "antd",
    "ssr",
    "server render",
    "redux",
    "hot reload",
    "express"
  ],
  "engines": {
    "node": "6.9.0",
    "npm": "3.10.8"
  },
  "license": "MIT",
  "scripts": {
    "start": "better-npm-run start-prod",
    "dev": "better-npm-run start-dev",
    "postinstall": "better-npm-run build:prod",
    "test": "tape -r babel-register tests/**/*.js"
  },
  "betterScripts": {
    "build:prod": {
      "command": "webpack --config webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "start-prod": {
      "command": "node ./bin/index.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "start-dev": {
      "command": "webpack --config webpack.devServer.config.js && babel-node ./bin/index.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    }
  },
  "dependencies": {
    "antd": "3.0.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.4",
    "babel-loader": "7.0.0",
    "babel-plugin-external-helpers": "6.8.0",
    "babel-plugin-import": "1.1.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "better-npm-run": "0.0.13",
    "compression": "1.6.1",
    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.23.1",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.8.5",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "less-vars-to-js": "1.2.0",
    "node-sass": "3.8.0",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.3.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.8",
    "react-transform-hmr": "1.0.0",
    "redux": "3.7.2",
    "redux-logger": "2.0.2",
    "redux-raven-middleware": "1.2.0",
    "redux-saga": "0.15.4",
    "redux-thunk": "2.2.0",
    "sass-loader": "3.2.1",
    "serialize-javascript": "1.1.2",
    "style-loader": "0.13.2",
    "webpack": "2.7.0",
    "webpack-dev-middleware": "1.12.2",
    "webpack-hot-middleware": "2.2.0",
    "webpack-node-externals": "1.5.4",
    "webpack-sentry-plugin": "1.14.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "tape": "4.7.0"
  }
}
